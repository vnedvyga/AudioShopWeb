<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <servlet>
        <servlet-name>GenresServlet</servlet-name>
        <servlet-class>ua.org.oa.nedvygav.servlets.GenresServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>ua.org.oa.nedvygav.servlets.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ArtistsServlet</servlet-name>
        <servlet-class>ua.org.oa.nedvygav.servlets.ArtistsServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AlbumsServlet</servlet-name>
        <servlet-class>ua.org.oa.nedvygav.servlets.AlbumsServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AudiosServlet</servlet-name>
        <servlet-class>ua.org.oa.nedvygav.servlets.AudiosServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CartServlet</servlet-name>
        <servlet-class>ua.org.oa.nedvygav.servlets.CartServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GenresServlet</servlet-name>
        <url-pattern>/genres</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ArtistsServlet</servlet-name>
        <url-pattern>/artists</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AlbumsServlet</servlet-name>
        <url-pattern>/albums</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AudiosServlet</servlet-name>
        <url-pattern>/audios</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CartServlet</servlet-name>
        <url-pattern>/cart</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
</web-app>